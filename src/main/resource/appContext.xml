<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	xmlns:p="http://www.springframework.org/schema/p">

	

	<bean id="helloBean" class="com.spring.Spring.HelloWorld"
		abstract="false" lazy-init="default" autowire="default">
		<property name="name" value="HelloWorld!!!"></property>
		
	</bean>
	
	<bean id="OutputHelper" class="com.spring.Spring.output.OutputHelper">
		<property name="gen" ref="JSONOutputGenerator"></property>
	</bean>
	
	<bean id="CVSOutputGenerator" class="com.spring.Spring.output.CVSOutputGenerator"></bean>
	<bean id="JSONOutputGenerator" class="com.spring.Spring.output.JSONOutputGenerator"></bean>
	
</beans>